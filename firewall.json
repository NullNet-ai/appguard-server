{
  "timeout": 1000,
  "default_policy": "allow",
  "cache": true,
  "expressions": [
    {
      "policy": "deny",
      "infix_tokens": [
        {
          "type": "predicate",
          "condition": "not_equal",
          "country": [
            "US",
            "PH",
            "IT",
            "CA"
          ]
        },
        {
          "type": "operator",
          "value": "or"
        },
        {
          "type": "predicate",
          "condition": "contains",
          "http_request_url": [
            ".php",
            ".env"
          ]
        },
        {
          "type": "operator",
          "value": "or"
        },
        {
          "type": "predicate",
          "condition": "contains",
          "source_ip": [
            "alias_name"
          ]
        }
      ]
    },
    {
      "policy": "allow",
      "infix_tokens": [
        {
          "type": "predicate",
          "condition": "equal",
          "org": [
            "EVOQUE"
          ]
        },
        {
          "type": "operator",
          "value": "and"
        },
        {
          "type": "predicate",
          "condition": "equal",
          "country": [
            "US"
          ]
        }
      ]
    }
  ]
}
